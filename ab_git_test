cd $KITCHEN_ROOT/source/projects/froyo/device/htc

#included for testing
rm -r desirec/
rm -r $KITCHEN_ROOT/source/projects/froyo/out/target/product/desirec

git clone git://github.com/gnarlyc/desirec.git

cd desirec/

mkdir -p $KITCHEN_ROOT/source/projects/froyo/vendor/htc/desirec/proprietary/
cp $KITCHEN_ROOT/prop/* $KITCHEN_ROOT/source/projects/froyo/vendor/htc/desirec/proprietary/
./extract-files.sh

cd ../../../
source build/envsetup.sh
lunch full_desirec-userdebug
make -j(grep -c processor /proc/cpuinfo)

